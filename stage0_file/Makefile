	rm -f *.o *.bin *.elf *.487 *.487 *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o

	rm -f *.o *.elf *.self *.488 *.bin *.488 *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
