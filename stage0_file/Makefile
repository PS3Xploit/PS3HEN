	rm -f *.o *.bin *.elf *.486 *.486 *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o

	rm -f *.o *.elf *.self *.487 *.bin *.487 *.map ../lv1/src/*.o ../debug/src/*.o ../lv2/src/*.o
